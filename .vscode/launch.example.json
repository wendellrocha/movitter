{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "movitter",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "args": ["--dart-define", "TMDB_API_KEY={{TMDB_API_KEY}}"]
    },
    {
      "name": "movitter (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "${workspaceFolder}/lib/main.dart",
      "args": ["--dart-define", "TMDB_API_KEY={{TMDB_API_KEY}}"]
    },
    {
      "name": "movitter (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "${workspaceFolder}/lib/main.dart",
      "args": ["--dart-define", "TMDB_API_KEY={{TMDB_API_KEY}}"]
    }
  ]
}
